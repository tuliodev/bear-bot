{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-restricted-syntax":"off",
        "import/no-dynamic-require":"off",
        "global-require":"off",
        "no-console":"off",
        "no-unused-vars":"warn",
        "no-useless-return":"off"
    }
}
